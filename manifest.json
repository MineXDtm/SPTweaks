{
  "manifest_version": 2,

  "name": "SPTweaks",
  "description": "Простой твикер для сайта проекта #SPWorlds. Переродился из личного проекта для удаления закрытых дел на суд.",
  "version": "1.3.2",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "three.js","three.js-master/examples/js/loaders/GLTFLoader.js","steve.gltf","alex.gltf","skybox.gltf",
    "poses/*","backgraund/*","styles/*","avatar_backgraund/*","decorations/*","emote_icon.png","saul_bg.png", "load_sticker.svg"
 ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["vizualno_ronunru.js"]
  }
}

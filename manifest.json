{
  "manifest_version": 2,
  "name": "SPTweaks",
  "description": "твикер для сайта проекта #SPWorlds. Переродился из личного проекта для удаления закрытых дел на суд.",
  "version": "2.0",
  "icons": {
    "48": "src/assets/logo/icon.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "background",
    "*://spworlds.ru/*"
  ],
  "{{chrome}}.action": {
    "default_popup": "popup.html"
  },
  "{{firefox}}.browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["src/assets/*","sptweaks.css","SegoeUI.ttf"],
  "background": {
    "{{chrome}}.scripts": ["vizualno_ronunru.js"],
    "persistent": true ,
    "{{firefox}}.scripts": [
      "vizualno_ronunru.js"
    ]
  }
}